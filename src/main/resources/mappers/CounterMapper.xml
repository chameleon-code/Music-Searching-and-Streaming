<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="kr.kina.mappers.CounterMapper">

<insert id="StreamingCounter" parameterType="kr.kina.domain.StreamingVO">
	insert into streaming
	(title, artist, album)
	values
	(#{title}, #{artist}, #{album})
</insert>


</mapper>   